{
  "name": "portfolio-site-server",
  "version": "1.0.0",
  "description": "Node.js server for my personal portfolio site, which uses React.js on the client",
  "private": true,
  "main": "server.js",
  "scripts": {
    "server": "nodemon index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Brandon Gregori",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "personal-portfolio-site": "file:client"
  }
}
